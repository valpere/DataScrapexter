        "acl",
        "kubernetes",
        "k8s",
        "helm",
        "istio",
        "envoy",
        "terraform",
        "ansible",
        "pprof",
        "cpu",
        "mem",
        "allocs",
        "mutex",
        "dockerfile",
        "dockerignore",
        "makefile",
        "yaml",
        "toml",
        "ini",
        "env",
        "dotenv",
        "gitignore",
        "editorconfig",
        "licensable",
        "vendored",
        "semver",
        "prerelease",
        "postprocess",
        "preprocessor",
        "nolint",
        "exportloopref",
        "ifshort",
