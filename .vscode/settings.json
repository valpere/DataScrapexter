        "ireturn",
        "contextcheck",
        "errchkjson",
        "valpere",
        "scraped",
        "scraping",
        "scrapers"
    ],
    "cSpell.allowCompoundWords": true,
    "cSpell.ignoreWords": [],
    "cSpell.customDictionaries": {
        "project-words": {
            "name": "project-words",
            "path": "${workspaceRoot}/.vscode/project-dictionary.txt",
            "description": "DataScrapexter project-specific words",
            "addWords": true
        }
    },
    
    "// Better Comments settings": "",
    "better-comments.tags": [
        {
            "tag": "!",
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "?",
            "color": "#3498DB",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "//",
            "color": "#474747",
            "strikethrough": true,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "todo",
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "*",
            "color": "#98C379",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        }
    ],
    
    "// Todo Tree settings": "",
    "todo-tree.tree.showScanModeButton": false,
    "todo-tree.highlights.enabled": true,
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "icon": "check",
            "type": "text",
            "foreground": "#FF8C00",
            "iconColour": "#FF8C00"
        },
        "FIXME": {
            "icon": "bug",
            "type": "text",
            "foreground": "#FF2D00",
            "iconColour": "#FF2D00"
        },
        "HACK": {
            "icon": "tools",
            "type": "text",
            "foreground": "#3498DB",
            "iconColour": "#3498DB"
        },
        "NOTE": {
            "icon": "note",
            "type": "text",
            "foreground": "#98C379",
            "iconColour": "#98C379"
        }
    },
    
    "// REST Client settings": "",
    "rest-client.environmentVariables": {
        "$shared": {
            "baseUrl": "http://localhost:8080",
            "apiVersion": "v1"
        },
        "local": {
            "baseUrl": "http://localhost:8080"
        },
        "dev": {
            "baseUrl": "https://dev-api.datascrapexter.com"
        },
        "prod": {
            "baseUrl": "https://api.datascrapexter.com"
        }
    },
    
    "// Thunder Client settings": "",
    "thunder-client.saveToWorkspace": true,
    "thunder-client.workspaceRelativePath": ".thunder-client",
    
    "// Error Lens settings": "",
    "errorLens.enabledDiagnosticLevels": [
        "error",
        "warning",
        "info"
    ],
    "errorLens.excludeBySource": [],
    "errorLens.delay": 300,
    "errorLens.followCursor": "allLines",
    
    "// SQL Tools settings": "",
    "sqltools.connections": [],
    "sqltools.useNodeRuntime": true,
    
    "// Makefile Tools settings": "",
    "makefile.extensionOutputFolder": "./.vscode",
    "makefile.launchConfigurations": [
        {
            "cwd": "${workspaceFolder}",
            "binaryPath": "${workspaceFolder}/bin/datascrapexter",
            "binaryArgs": []
        }
    ],
    "makefile.makePath": "make",
    
    "// Shell format settings": "",
    "shellformat.flag": "-i 4 -ci -sr",
    "shellformat.effectLanguages": [
        "shellscript",
        "dockerfile",
        "ignore",
        "gitignore",
        "dotenv",
        "hosts",
        "properties",
        "jvmoptions"
    ],
    
    "// Perl settings": "",
    "perl.perlInc": [],
    "perl.enableWarnings": true,
    "perl.perltidyProfile": "",
    "perl.perlcriticProfile": "",
    
    "// Live Share settings": "",
    "liveshare.presence": true,
    "liveshare.showInStatusBar": "whileCollaborating",
    
    "// GitLens settings": "",
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.statusBar.enabled": false,
    
    "// Project Manager settings": "",
    "projectManager.git.baseFolders": [
        "${workspaceFolder}"
    ],
    
    "// Remote development settings": "",
    "remote.autoForwardPorts": true,
    "remote.restoreForwardedPorts": true,
    
    "// Security settings": "",
    "security.workspace.trust.untrustedFiles": "open",
    "security.workspace.trust.banner": "always",
    
    "// Telemetry settings": "",
    "telemetry.telemetryLevel": "off",
    
    "// Update settings": "",
    "update.mode": "manual",
    
    "// Window settings": "",
    "window.confirmBeforeClose": "keyboardOnly",
    
    "// Workbench settings": "",
    "workbench.editor.enablePreview": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 10,
    "workbench.editor.tabCloseButton": "right",
    "workbench.editor.tabSizing": "shrink",
    
    "// Explorer settings": "",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    
    "// Breadcrumbs settings": "",
    "breadcrumbs.enabled": true,
    
    "// Outline settings": "",
    "outline.showVariables": true,
    "outline.showFunctions": true,
    "outline.showConstants": true,
    "outline.showStructs": true,
    "outline.showInterfaces": true
}
